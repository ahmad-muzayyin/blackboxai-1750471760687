# API Configuration
REACT_APP_API_URL=http://localhost:5000/api/v1
REACT_APP_API_TIMEOUT=30000

# Feature Flags
REACT_APP_FEATURE_BANTUAN_SOSIAL=true
REACT_APP_FEATURE_APBDES=true
REACT_APP_FEATURE_SURAT_ONLINE=true
REACT_APP_FEATURE_PENGADUAN=true

# Authentication
REACT_APP_JWT_STORAGE_KEY=desa_digital_token
REACT_APP_REFRESH_TOKEN_KEY=desa_digital_refresh_token

# File Upload
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# UI Configuration
REACT_APP_THEME_PRIMARY_COLOR=#1976d2
REACT_APP_THEME_SECONDARY_COLOR=#dc004e
REACT_APP_ROWS_PER_PAGE=10

# Analytics
REACT_APP_ENABLE_ANALYTICS=true

# Error Reporting
REACT_APP_ENABLE_ERROR_REPORTING=true

# Cache
REACT_APP_CACHE_DURATION=3600000
